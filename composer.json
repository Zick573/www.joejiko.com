{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": ["framework", "laravel"],
    "require": {
        "laravel/framework": "4.0.*",
        "way/generators": "dev-master",
        "hybridauth/hybridauth": "*",
        "michelf/php-markdown": "1.3",
        "imagine/Imagine": "*",
        "sybio/image-workshop": "2.*",
        "knplabs/knp-snappy": "*",
        "kriswallsmith/assetic": "*",
        "fabpot/goutte": "*",
        "nesbot/carbon": "*",
        "Respect\/Validation": "0.4.*",
        "codeguy/upload": "*",
        "mjaschen/phpgeo": "*",
        "mobiledetect/mobiledetectlib": "*",
        "evert/google-api-php-client": "dev-master",
        "soramugi/google-drive-client-php": "dev-master",
        "mashape/unirest-php": "dev-master",
        "liebig/cron": "dev-master",
        "duvanmonsa/php-query": "dev-master"
    },
    "require-dev": {
        "way/guard-laravel": "dev-master"
    },
    "autoload": {
        "classmap": [
            "app/commands",
            "app/controllers",
            "app/libraries",
            "app/models",
            "app/database/migrations",
            "app/database/seeds",
            "app/tests/TestCase.php"
        ],
        "psr-0": {"Unirest":"lib/"},
        "files": [
            "app/libraries/helpers/globals.php"
        ]
    },
    "scripts": {
        "pre-update-cmd": [
            "php artisan clear-compiled"
        ],
        "post-install-cmd": [
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev"
}